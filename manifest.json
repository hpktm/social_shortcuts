{
  "manifest_version": 2,

  "name": "Link Launcher",
  "description": "Quick launch Pant Exxtension",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
  	//"activeTab"
  	"storage"
  ]
}
